class Guest:
    pass

g_1 = Guest()
g_1.first = "Eve"
g_1.last = "Dela Cruz"
g_1.interest = "Anime"
g_1.phone = 12345
g_1.crush = ""
print(f"Hi there, {g_1.first} {g_1.last}!")

g_2 = Guest()

g_2.first = "Adam"
g_2.last = "Perez"
g_2.interest = "Russian literature"
g_2.phone = 789456
g_2.crush = ""
print(f"Hi there, {g_2.first} {g_2.last}!")
