class Employee:
    def __init__(self,name,email):
        self.name = name
        self.email = email
        
class TechDepartment (Employee):
    def display(self):
        print(f"Name: {self.name}")
        print(f"Email: {self.email}")

emp = TechDepartment ("Lisa", "lisa12@gmail.com")

emp.display() 
## calls the subclass "display" referencing to emp
## subclass "display" inherited the parent class
